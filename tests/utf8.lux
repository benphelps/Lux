    говорити() println("Гав!");
}

class Кішка < Тварина {
    говорити() println("мрр мрр");
}

let пес = Пес();
let кішка = Кішка();
пес.їсти("кістку");
кішка.їсти("рибу");
пес.говорити();
кішка.говорити();
